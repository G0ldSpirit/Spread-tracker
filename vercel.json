{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/markets.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/markets",
      "dest": "/api/markets.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
